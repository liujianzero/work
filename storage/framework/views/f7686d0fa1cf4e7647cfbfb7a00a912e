<script>
    $(".banner-item").on("click",function(){
        $(".banner-item").removeClass("selected");
        $(this).addClass("selected");
    });

    $(function(){
        $(".sub-popup-box").css("display","none");
//        $(".hongbao-box").css("display","none");

    });

    function submit(){
        var number=0;
        var arr=["sex","age","major","camera","life","software","industry"];
//        for(var a in arr){
//            var checkedNumber=$('input:radio[name="'+arr[a]+'"]:checked').val();
//            number+=parseInt(checkedNumber);
//            if(typeof(checkedNumber)=="undefined"){
//                alert("你还有题目没有选择");
//                return false;
//            }
//        }
        if($(".sub-popup-box").css("display")=="none"){
            $(".sub-popup-box").css("display","block");
            $(".sub-content span").html(number+"天");
        }else{
            $(".sub-popup-box").css("display","none");
        }
    }

        //    关闭测试
        $(".popup-head .close").on("click",function(){
            $("#hide-box").css("display","none");
        });

        //    关闭测试结果
        $(".sub-popup-head .close").on("click",function(){
            $(".sub-popup-box").css("display","none");
        });
        //关闭红包
        $(".hongbao-box .close").on("click",function(){
            $(".hongbao-box").css("display","none");
        });


</script>
<script type="text/javascript">
    var oTest1 = document.getElementsByClassName("test1")[0];
    var aRadio = document.querySelectorAll(".radio-box input[type='radio']");//获取所有inputradio
    var oClosebox = document.getElementById("close-box");
    var oHidebox = document.getElementById("hide-box");
    var uid = parseInt($('.uid').val());

        var rand = <?php echo e($randnum); ?>;
        if( rand == 0) {
            oTest1.onclick = function () {
                if(uid == 0 ){
                    layer.msg("请先登录！");
                }else {
                    oHidebox.style.display = "block";
                    $(".hongbao-box").css("display", "none");
                }
            }
        }else{
            var is_show = <?php echo e($is_show); ?>

            if(is_show == 1){
                oHidebox.style.display = "block";
                $(".hongbao-box").css("display","block");
            }else {
                $('.test1').click(function(){

                    oHidebox.style.display = "block";
                    $(".hongbao-box").css("display","block");
                });
            }

//        oTest1.onclick = function () {
////
//        }
        }


    oClosebox.onclick = function(){
        for (var i = 0; i < aRadio.length; i++) {
            aRadio[i].checked = false;//吧radio的checked属性改变
        }
    }
</script>
